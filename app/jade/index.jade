doctype
html
  head
    meta(charset='utf-8')
    title list of pages
    link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Open+Sans:200,400,600,700&amp;subset=cyrillic')
    link(rel='stylesheet', href='css/pages.css')
  body
    ul.pages
      -
        var pages = ["atoms", "molecules", "organisms", "frontpage", "categories", "place"];
      each item in pages
        li.pages__item
          a.pages__link(href='#{item}.html')= item